
@{
    ViewBag.Title = "Dashboard";
}

@{
    double media;
    double minimo;
    double maximo;

    if (ViewBag.media != null)
    {
        media = ViewBag.media;
        minimo = ViewBag.minimo;
        maximo = ViewBag.maximo;
    }
    else
    {
        media = 0.00;
        minimo = 0.00;
        maximo = 0.00;
